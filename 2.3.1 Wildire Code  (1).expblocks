or5 = motor(PORT5, true);\n\n\n\n// generating and setting random seed\nvoid initializeRandomSeed(){\n  wait(100,msec);\n  double xAxis = BrainInertial.acceleration(xaxis) * 1000;\n  double yAxis = BrainInertial.acceleration(yaxis) * 1000;\n  double zAxis = BrainInertial.acceleration(zaxis) * 1000;\n  // Combine these values into a single integer\n  int seed = int(\n    xAxis + yAxis + zAxis\n  );\n  // Set the seed\n  srand(seed);\n}\n\n\n\nvoid vexcodeInit() {\n\n  // Initializing random seed.\n  initializeRandomSeed(); \n}\n\n\n// Generated code.\n\n// Include the EXP Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable, debug, row, col;\n\nbool debugg;\n\nfloat svdd[5][5];\nfloat svd[5];\nfloat roww[2];\n\nevent message1 = event();\nevent FWD = event();\nevent LT = event();\nevent RT = event();\nevent backword = event();\n\n// \"when I receive backword\" hat block\nvoid onevent_backword_0() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"backword\");\n  }\n  else {\n    Motor1.spinFor(reverse, 412.0, degrees, true);\n  }\n}\n\n// \"when I receive backword\" hat block\nvoid onevent_backword_1() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"backword\");\n  }\n  else {\n    Motor5.spinFor(reverse, 412.0, degrees, true);\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  FWD.broadcastAndWait();\n  FWD.broadcastAndWait();\n  FWD.broadcastAndWait();\n  backword.broadcastAndWait();\n  RT.broadcastAndWait();\n  FWD.broadcastAndWait();\n  FWD.broadcastAndWait();\n  RT.broadcastAndWait();\n  FWD.broadcastAndWait();\n  FWD.broadcastAndWait();\n  LT.broadcastAndWait();\n  FWD.broadcastAndWait();\n  FWD.broadcastAndWait();\n  return 0;\n}\n\n// \"when I receive FWD\" hat block\nvoid onevent_FWD_0() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"forward\");\n  }\n  else {\n    Motor1.spinFor(forward, 412.0, degrees, true);\n  }\n}\n\n// \"when I receive FWD\" hat block\nvoid onevent_FWD_1() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"forward\");\n  }\n  else {\n    Motor5.spinFor(forward, 412.0, degrees, true);\n  }\n}\n\n// \"when I receive LT\" hat block\nvoid onevent_LT_0() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"turn left\");\n  }\n  else {\n    Motor1.spinFor(reverse, 241.0, degrees, true);\n  }\n}\n\n// \"when I receive LT\" hat block\nvoid onevent_LT_1() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"turn left\");\n  }\n  else {\n    Motor5.spinFor(forward, 241.0, degrees, true);\n  }\n}\n\n// \"when I receive RT\" hat block\nvoid onevent_RT_0() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"turn right\");\n  }\n  else {\n    Motor5.spinFor(reverse, 241.0, degrees, true);\n  }\n}\n\n// \"when I receive RT\" hat block\nvoid onevent_RT_1() {\n  if (debugg) {\n    Brain.Screen.newLine();\n    Brain.Screen.print(\"turn right\");\n  }\n  else {\n    Motor1.spinFor(forward, 241.0, degrees, true);\n  }\n}\n\n\nint main() {\n  // Initializing Robot Configuration. DO NOT REMOVE!\n  vexcodeInit();\n\n  // register event handlers\n  backword(onevent_backword_0);\n  backword(onevent_backword_1);\n  FWD(onevent_FWD_0);\n  FWD(onevent_FWD_1);\n  LT(onevent_LT_0);\n  LT(onevent_LT_1);\n  RT(onevent_RT_0);\n  RT(onevent_RT_1);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // Wait for ADI sensors to initialize\n  wait(100, msec);\n\n\n  whenStarted1();\n}","target":"Physical"}